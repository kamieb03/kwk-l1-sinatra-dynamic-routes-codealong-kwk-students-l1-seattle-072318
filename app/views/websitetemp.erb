}
/* Create a Parallax Effect */
.bgimg-1, .bgimg-2, .bgimg-3, .bgimg-4 {
    background-attachment: fixed;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}
/* First image (Elephant) */
.bgimg-1 {
    background-image: url("http://blog.nationalgeographic.org/wp-content/uploads/2013/08/Ed-and-Erin.jpg");
    min-height: 100%;
}
/* Second image (Snow Leopard) */
.bgimg-2 {
    background-image: url("https://img.huffingtonpost.com/asset/562846ab1200002e007e5b53.jpeg?ops=crop_0_452_5315_3215,scalefit_720_noupscale");
    min-height: 400px;
}
/* Third image (Panda) */
.bgimg-3 {
    background-image: url("https://www.natgeokids.com/wp-content/uploads/2014/04/panda-facts-6-1.jpg");
    min-height: 400px;
}
/* Fourth image (Gorilla) */
.bgimg-4 {
    background-image: url("https://3sn4dm1qd6i72l8a4r2ig7fl-wpengine.netdna-ssl.com/wp-content/uploads/2016/12/gorilla_130424_lulu_andi_henry_merry_ZA_8752.jpg");
    min-height: 400px;
}
.w3-wide {letter-spacing: 10px;}
.w3-hover-opacity {cursor: pointer;}
/* Turn off parallax scrolling for tablets and phones */
@media only screen and (max-device-width: 1024px) {
    .bgimg-1, .bgimg-2, .bgimg-3, .bgimg-4 {
        background-attachment: scroll;
    }
}
</style>
